    "import re\n",
    "\n",
    "plaque = input()\n",
    "prog = re.findall('^[A-Z]{2}-\\d{3}-[A-Z]{2}$', plaque)\n",
    "\n",
    "if len(prog) == plaque:       # ou !=0\n",
    "    print(\"ceci est une plaque\")\n",
    "    \n",
    "else:\n",
    "    print(\"il ne s'agit pas d'une plaque\")"
